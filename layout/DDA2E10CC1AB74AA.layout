	<!--
    ***************************************************
        Embed/EmbedShared16
    ***************************************************
    -->
  <WidgetLook name="Embed/EmbedShared16">
    <ImagerySection name="frame">
      <FrameComponent>
        <Area>
          <Dim type="LeftEdge" >
            <AbsoluteDim value="0" />
          </Dim>
          <Dim type="TopEdge" >
            <AbsoluteDim value="0" />
          </Dim>
          <Dim type="Width" >
            <UnifiedDim scale="1" type="Width" />
          </Dim>
          <Dim type="Height" >
            <UnifiedDim scale="1" type="Height" />
          </Dim>
        </Area>
        <Image type="TopLeftCorner" imageset="Embed" image="embed16TopLeft" />
        <Image type="TopRightCorner" imageset="Embed" image="embed16TopRight" />
        <Image type="BottomLeftCorner" imageset="Embed" image="embed16BottomLeft" />
        <Image type="BottomRightCorner" imageset="Embed" image="embed16BottomRight" />
        <Image type="LeftEdge" imageset="Embed" image="embed16Left" />
        <Image type="RightEdge" imageset="Embed" image="embed16Right" />
        <Image type="TopEdge" imageset="Embed" image="embed16Top" />
        <Image type="BottomEdge" imageset="Embed" image="embed16Bottom" />
        <ColourRectProperty name="FrameColours" />
      </FrameComponent>
    </ImagerySection>
    <ImagerySection name="background">
      <ImageryComponent>
        <Area>
          <Dim type="LeftEdge" >
            <ImageDim imageset="Embed" image="embed16Left" dimension="Width" />
          </Dim>
          <Dim type="TopEdge" >
            <ImageDim imageset="Embed" image="embed16Top" dimension="Height" />
          </Dim>
          <Dim type="RightEdge" >
            <UnifiedDim scale="1" type="RightEdge">
              <DimOperator op="Subtract">
                <ImageDim imageset="Embed" image="embed16Right" dimension="Width" />
              </DimOperator>
            </UnifiedDim>
          </Dim>
          <Dim type="BottomEdge" >
            <UnifiedDim scale="1" type="BottomEdge">
              <DimOperator op="Subtract">
                <ImageDim imageset="Embed" image="embed16Bottom" dimension="Height" />
              </DimOperator>
            </UnifiedDim>
          </Dim>
        </Area>
        <Image imageset="Embed" image="embed16Backdrop" />
        <!--<ColourRectProperty name="BackgroundColours" />-->
        <Colours topLeft="FFFFFFFF" topRight="FFFFFFFF" bottomLeft="FFFFFFFF" bottomRight="FFFFFFFF" />
        <VertFormat type="Stretched" />
        <HorzFormat type="Stretched" />
      </ImageryComponent>
    </ImagerySection>
    <ImagerySection name="background_noframe">
      <ImageryComponent>
        <Area>
          <Dim type="LeftEdge">
            <AbsoluteDim value="0" />
          </Dim>
          <Dim type="TopEdge">
            <AbsoluteDim value="0" />
          </Dim>
          <Dim type="RightEdge">
            <UnifiedDim scale="1" type="RightEdge" />
          </Dim>
          <Dim type="BottomEdge">
            <UnifiedDim scale="1" type="BottomEdge" />
          </Dim>
        </Area>
        <Image imageset="Embed" image="embed16Backdrop" />
        <!--<ColourRectProperty name="BackgroundColours" />-->
        <Colours topLeft="FFFFFFFF" topRight="FFFFFFFF" bottomLeft="FFFFFFFF" bottomRight="FFFFFFFF" />
        <VertFormat type="Stretched" />
        <HorzFormat type="Stretched" />
      </ImageryComponent>
    </ImagerySection>
  </WidgetLook>  

